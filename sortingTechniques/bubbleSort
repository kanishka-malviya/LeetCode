// Bubble sort sorts by putting the largest element on the right most position of the array while arranging in ascending order
function bubblesort(array) {
    for (let i = array.length-1; i >= 0; i--) {
        for (let j = i-1; j >= 0; j--) {
            if (array[j] > array[i]) {
                let temp = array[j];
                array[j] = array[i];
                array[i] = temp;
            }
        }
    }
    return array;
}

let array = [3, 5, 2, 9, 1];
let ans = bubblesort(array);
console.log(ans);
