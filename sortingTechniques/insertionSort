function sort(arr) {
    for (let i = 0; i < array.length; i++) {
        let j = i;

        while(j >= 0 && arr[j-1] > arr[j]) {
            let temp = arr[j-1];
            arr[j-1] = arr[j];
            arr[j] = temp;
            j--;
        }
    }

    return arr;
}

let array = [6, 3, 8, 1, 9];
let ans = sort(array);
console.log(ans);
